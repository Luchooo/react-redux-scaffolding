{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "global-require": 0,
    "comma-dangle": 0,
    "arrow-body-style": 0,
    "space-before-function-paren": 0,
    "import/no-extraneous-dependencies": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0
  }
}
